                               QUERY PLAN                                
-------------------------------------------------------------------------
 Limit  (cost=100.00..101.00 rows=1 width=64)
   ->  Foreign Scan on e_zipsjson  (cost=100.00..101.00 rows=1 width=64)
         Quasar query: SELECT loc, loc AS locb FROM zips
(3 rows)

/* Pushdown regex operators */
EXPLAIN SELECT * FROM e_zips WHERE "state" LIKE 'A%' LIMIT 3;
                                      QUERY PLAN                                      
--------------------------------------------------------------------------------------
 Limit  (cost=100.00..101.00 rows=1 width=80)
   ->  Foreign Scan on e_zips  (cost=100.00..101.00 rows=1 width=80)
         Filter: (state ~~ 'A%'::text)
         Quasar query: SELECT _id, city, pop, state FROM zips WHERE (state LIKE 'A%')
(4 rows)

EXPLAIN SELECT * FROM e_zips WHERE "city" !~~ 'B%' LIMIT 3;
                                       QUERY PLAN                                        
-----------------------------------------------------------------------------------------
 Limit  (cost=100.00..101.00 rows=1 width=80)
   ->  Foreign Scan on e_zips  (cost=100.00..101.00 rows=1 width=80)
         Filter: ((city)::text !~~ 'B%'::text)
         Quasar query: SELECT _id, city, pop, state FROM zips WHERE (city NOT LIKE 'B%')
(4 rows)

/* pushdown math operators */
EXPLAIN SELECT * FROM e_zips WHERE pop > 1000 AND pop + pop <= 10000 LIMIT 3;
                                                 QUERY PLAN                                                 
------------------------------------------------------------------------------------------------------------
 Limit  (cost=100.00..101.00 rows=1 width=80)
   ->  Foreign Scan on e_zips  (cost=100.00..101.00 rows=1 width=80)
         Filter: ((pop > 1000) AND ((pop + pop) <= 10000))
         Quasar query: SELECT _id, city, pop, state FROM zips WHERE (pop > 1000) AND ((pop + pop) <= 10000)
(4 rows)

/* join zips and zipsjson */
EXPLAIN SELECT city,pop,state,loc FROM e_zips JOIN e_zipsjson ON e_zips._id = e_zipsjson._id LIMIT 3;
                                  QUERY PLAN                                   
-------------------------------------------------------------------------------
 Limit  (cost=200.00..202.01 rows=1 width=80)
   ->  Nested Loop  (cost=200.00..202.01 rows=1 width=80)
         Join Filter: ((e_zips._id)::text = (e_zipsjson._id)::text)
         ->  Foreign Scan on e_zips  (cost=100.00..101.00 rows=1 width=80)
               Quasar query: SELECT _id, city, pop, state FROM zips
         ->  Foreign Scan on e_zipsjson  (cost=100.00..101.00 rows=1 width=64)
               Quasar query: SELECT _id, loc FROM zips
(7 rows)

