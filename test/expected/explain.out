CREATE SERVER e_quasar FOREIGN DATA WRAPPER quasar_fdw OPTIONS (server 'http://localhost:8080', path '/local/quasar');
CREATE FOREIGN TABLE e_zips(_id varchar, city varchar, pop integer, state char(2))
       SERVER e_quasar OPTIONS (table 'zips');
CREATE FOREIGN TABLE e_zipsloc(loc numeric[2])
       SERVER e_quasar OPTIONS (table 'zips');
CREATE FOREIGN TABLE e_zipsjson(_id varchar, loc json, locb jsonb OPTIONS (map 'loc'))
       SERVER e_quasar OPTIONS (table 'zips');
CREATE FOREIGN TABLE e_nested(a varchar OPTIONS (map 'topObj.midObj.botObj.a'),
                              b varchar OPTIONS (map 'topObj.midObj.botObj.b'),
                              c varchar OPTIONS (map 'topObj.midObj.botObj.c'))
       SERVER e_quasar OPTIONS (table 'nested');
/* Select */
/* Basic selection with limit */
EXPLAIN SELECT * FROM e_zips LIMIT 3;
                             QUERY PLAN                              
---------------------------------------------------------------------
 Limit  (cost=100.00..101.00 rows=1 width=80)
   ->  Foreign Scan on e_zips  (cost=100.00..101.00 rows=1 width=80)
         Quasar query: SELECT _id, city, pop, state FROM zips
(3 rows)

/* Select less fields than exist */
EXPLAIN SELECT city FROM e_zips LIMIT 1;
                             QUERY PLAN                              
---------------------------------------------------------------------
 Limit  (cost=100.00..101.00 rows=1 width=32)
   ->  Foreign Scan on e_zips  (cost=100.00..101.00 rows=1 width=32)
         Quasar query: SELECT city FROM zips
(3 rows)

/* Basic WHERE clause */
EXPLAIN SELECT * FROM e_zips WHERE "state" = 'CO' LIMIT 2;
                                    QUERY PLAN                                     
-----------------------------------------------------------------------------------
 Limit  (cost=100.00..101.00 rows=1 width=80)
   ->  Foreign Scan on e_zips  (cost=100.00..101.00 rows=1 width=80)
         Filter: (state = 'CO'::bpchar)
         Quasar query: SELECT _id, city, pop, state FROM zips WHERE (state = 'CO')
(4 rows)

/* Nested selection */
EXPLAIN SELECT * FROM e_nested LIMIT 1;
                                                           QUERY PLAN                                                           
--------------------------------------------------------------------------------------------------------------------------------
 Limit  (cost=100.00..101.00 rows=1 width=96)
   ->  Foreign Scan on e_nested  (cost=100.00..101.00 rows=1 width=96)
         Quasar query: SELECT topObj.midObj.botObj.a AS a, topObj.midObj.botObj.b AS b, topObj.midObj.botObj.c AS c FROM nested
(3 rows)

/* less fields than in relation, with one in a WHERE clause */
EXPLAIN SELECT city FROM e_zips WHERE "state" = 'CO' LIMIT 1;
                               QUERY PLAN                                
-------------------------------------------------------------------------
 Limit  (cost=100.00..101.00 rows=1 width=32)
   ->  Foreign Scan on e_zips  (cost=100.00..101.00 rows=1 width=32)
         Filter: (state = 'CO'::bpchar)
         Quasar query: SELECT city, state FROM zips WHERE (state = 'CO')
(4 rows)

EXPLAIN SELECT city,pop FROM e_zips WHERE pop % 2 = 1 LIMIT 3;
                               QUERY PLAN                               
------------------------------------------------------------------------
 Limit  (cost=100.00..101.00 rows=1 width=36)
   ->  Foreign Scan on e_zips  (cost=100.00..101.00 rows=1 width=36)
         Filter: ((pop % 2) = 1)
         Quasar query: SELECT city, pop FROM zips WHERE ((pop % 2) = 1)
(4 rows)

